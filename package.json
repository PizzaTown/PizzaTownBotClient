{
  "name": "pizzatown-client",
  "version": "1.0.0",
  "description": "PizzaTown's Discord Client System",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "cross-env-shell babel src -d dist --source-maps --extensions '.ts'",
    "build": "npm run clean && npm run compile",
    "typecheck": "npx tsc --p .",
    "build-typecheck": "npm run typecheck && npm run build",
    "start": "npm run build-typecheck && node ./dist/index.js"
  },
  "author": "Rocky43007",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.9.4",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "jest": "^27.2.1",
    "node-fetch": "^3.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
